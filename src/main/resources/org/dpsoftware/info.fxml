<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.dpsoftware.InfoController">
   <padding>
      <Insets left="20.0" right="20.0" top="20.0">
         <bottom>20.0</bottom>
      </Insets>
   </padding>
   <Label text="Info View"/>
   <Button fx:id="closeButton" onMouseClicked="#onMouseClickedCloseBtn" text="Close"/>
</VBox>
